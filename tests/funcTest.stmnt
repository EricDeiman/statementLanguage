hello(which, name1, name2) {
  if(which) {
    print "Hello" name1;
  }
  else {
    print "Hello" name2;
  }
  return 0;
}

hello(kid1 < kid2, kid1, kid2);
hello(kid1 > kid2, kid1, kid2);

